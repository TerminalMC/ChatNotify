{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${mod_group}",
        "id": "${mod_id}",
        "version": "${version}",
        "provides": [],
        "entrypoints": {
            "main": [],
            "client": "notryken.chatnotify.ChatNotifyFabric",
            "server": [],
            "modmenu": "notryken.chatnotify.gui.screen.ModMenuIntegration"
        },
        "depends": [
            {
                "id": "minecraft",
                "versions": "~${minecraft_version}"
            },
            {
                "id": "java",
                "versions": ">=17"
            }
        ],
        "recommends": [
            {
                "id": "modmenu",
                "versions": "${modmenu_version}"
            }
        ],
        "suggests": [],
        "conflicts": [],
        "breaks": [],
        "intermediate_mappings": "net.fabricmc:intermediary",

        "metadata": {
            "name": "${mod_name}",
            "description": "${mod_description}",
            "contact": {
                "homepage": "${homepage_url}",
                "sources": "${sources_url}",
                "issues": "${issues_url}"
            },
            "contributors": {
                "NotRyken": "Owner"
            },
            "license": "${mod_license}",
            "icon": "assets/${mod_id}/icon.png"
        }
    },
    "mixin": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "access_widener": "${mod_id}.accesswidener",
    "minecraft": {
        "environment": "${mod_environment}"
    },
    "modmenu": {
        "links": {
            "Contact": "${contact_url}"
        }
    },
    "chatnotify": {
        "links": {
            "formatSpec": "https://github.com/QuiltMC/rfcs/blob/main/specification/0002-quilt.mod.json.md"
        }
    }
}